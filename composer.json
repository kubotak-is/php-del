{
    "name": "kubotak-is/php-del",
    "description": "Tool to remove code based on specific comments.",
    "authors": [
        {
            "name": "kubotak-is",
            "email": "kenjiro.kubota@gmail.com"
        }
    ],
    "license": ["MIT"],
    "scripts": {
        "test": [
            "phpunit"
        ]
    },
    "require": {
        "php": ">=7.4",
         "ext-mbstring": "*",
        "league/climate": "^3.7"
    },
    "bin": [
